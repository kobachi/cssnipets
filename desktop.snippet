<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Desktopのパス</Title>
			<Shortcut>desktop</Shortcut>
			<Description>Environment.GetFolderPath(Environment.SpecialFolder.Desktop) に対するコード スニペット</Description>
			<Author>kobachi</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="false">
					<ID>SystemEnvironment</ID>
					<Function>SimpleTypeName(System.Environment)</Function>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[$SystemEnvironment$.GetFolderPath($SystemEnvironment$.SpecialFolder.Desktop)]]></Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
